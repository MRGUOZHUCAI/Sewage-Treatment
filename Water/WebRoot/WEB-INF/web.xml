    <servlet-class>com.apache.Algorithm.knn</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>knn</servlet-name>
    <url-pattern>/knn.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>doInf</servlet-name>
    <servlet-class>com.apache.DataInput.doInf</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>doInf</servlet-name>
    <url-pattern>/doInf.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>test</servlet-name>
